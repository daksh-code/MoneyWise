container_commands:
  00_makemigrations:
    command: "source /var/app/stockenv/*/bin/activate && python3 manage.py makemigrations --noinput"
    leader_only: true
  01_migrate:
    command: "source /var/app/stockvenv/*/bin/activate && python3 manage.py migrate --noinput"
    leader_only: true
  02_superuser:
    command: "source /var/app/stockvenv/*/bin/activate && python3 manage.py createsu"
    leader_only: true
